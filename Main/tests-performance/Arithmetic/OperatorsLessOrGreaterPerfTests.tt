			continue;

		WriteRegionLine(op.Name);
		EmitGreaterOrLessOpCase(op, "int", "Int");
		WriteLine();
		EmitGreaterOrLessOpCase(op, "int?", "NullableInt");
		WriteLine();
		EmitGreaterOrLessOpCase(op, "AttributeTargets", "Enum");
		WriteLine();
		EmitGreaterOrLessOpCase(op, "AttributeTargets?", "NullableEnum");
		WriteLine();
		EmitGreaterOrLessOpCase(op, "DateTime?", "NullableDateTime");
		WriteLine();
		EmitGreaterOrLessOpCase(op, "string", "String", "string.CompareOrdinal(a, b) " + op.Sign + " 0");
	}

	FlushRegions();
#>
	}
