		if (op.OpType == OpType.Unary)
		{
			EmitUnaryOpCase(op, "int", "Int");
		}
		else
		{
			EmitBinaryOpCase(op, "int", "Int");
			if (!op.IntOnly)
			{
				WriteLine();
				EmitBinaryOpCase(op, "double?", "NullableDouble");
			}
		}
	}
	FlushRegions();
#>
	}
